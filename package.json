{
  "name": "rs-yaxt-client",
  "version": "3.0.0",
  "description": "riot standard components for yaxt client",
  "main": "index.html",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --hot",
    "babel:watch": "babel js --watch --out-dir dist --source-maps",
    "babel:compile": "babel js --out-dir dist --source-maps",
    "watch": "riot --type es6 -w ./client/tags ./client/dist",
    "compile-tags": "riot --type babel ./tags ./lib/tags.js",
    "build": "cross-env NODE_ENV=product webpack --config webpack.config.js --display-optimization-bailout --display-error-details",
    "build2": "webpack-dashboard -- webpack --config webpack.config.js --display-optimization-bailout --display-error-details -p",
    "build-debug": "webpack --config webpack.config.debug.js",
    "format": "standard --fix ./src/js/**/*.js"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.1.1",
    "cross-env": "^7.0.2",
    "dexie": "^2.0.1",
    "echarts": "^3.8.5",
    "fuse.js": "^3.2.0",
    "openlayers": "^4.6.4",
    "papaparse": "^4.3.6",
    "riot": "^3.7.4",
    "socket.io-client": "^2.0.4",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-plugin-html": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "js-md5": "^0.7.3",
    "node-sass": "^4.7.2",
    "postcss": "^6.0.14",
    "postcss-loader": "^2.0.9",
    "precss": "^2.0.0",
    "riot-compiler": "^3.4.0",
    "riot-hot-reload": "^1.0.0",
    "riot-tag-loader": "^2.0.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.7",
    "workbox-webpack-plugin": "^2.1.2"
  },
  "keywords": [
    "indoor",
    "locating",
    "map",
    "app"
  ],
  "standard": {
    "globals": [
      "PAGE_SIZE",
      "io",
      "riot",
      "xbus",
      "xdata",
      "xsocket",
      "localStorage",
      "performance",
      "requestAnimationFrame",
      "cancelAnimationFrame",
      "opts"
    ]
  },
  "author": "HANK WANG",
  "license": "MIT"
}
